        ],
        "main": "dist/js/materialize",
        "shim": {
          "dist/js/materialize": {
            "deps": [
              "jquery"
            ],
            "exports": "$"
          }
        },
        "dependencies": {
          "jquery": "github:components/jquery",
          "css": "github:systemjs/plugin-css"
        }
